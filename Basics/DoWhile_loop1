public class do_while1 {
    public static void main(String[] args) {
        int i=0;
        do{
            System.out.print(i);
            i++;
        }while (i<11);
        System.out.println();

        int j=10;
        do {
            System.out.println("sneha");
        }while (j>10);
    }
}
